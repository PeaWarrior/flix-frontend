nge\n        })\n      })\n      .then(resp => resp.json())\n      .then(data => {\n        setState(prevState => ({\n          ...prevState,\n          ...data\n        }))\n      })\n  }\n\n  const handleClickLikes = () => {\n    updateLikes('like');\n  };\n\n  const handleClickDislikes = () => {\n    updateLikes('dislike');\n  };\n\n  return (\n    <StyledMovieSection>\n      <img src={state.poster} alt=\"\" />\n      <div>\n        <h1>{state.title}</h1>\n        <hr/>\n        <StyledMetaList>{renderMetaData()}</StyledMetaList>\n        <StyledButton onClick={handleClickLikes} color={'green'}>\n          <FaRegThumbsUp />\n          {state.likes}\n        </StyledButton> \n\n        <StyledButton onClick={handleClickDislikes} color={'red'}>\n          <FaRegThumbsDown />\n          {state.dislikes}\n        </StyledButton>\n        <p>{state.overview}</p>\n      </div>\n    </StyledMovieSection>\n  )\n}\n\nconst initialState = {\n  movie_id: null,\n  title: null,\n  director: null,\n  overview: null,\n  runtime: null,\n  released: null,\n  backdrop: null,\n  poster: null,\n}\n\nexport default MoviePage;","/Users/jacksonchen/yearone/flix-frontend/src/components/MoviePage/MoviePage.styled.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/utilities/theme.js",[],["73","74"],"/Users/jacksonchen/yearone/flix-frontend/src/components/SearchPage/SearchPage.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/SearchPage/SearchPage.styled.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/utilities/GlobalStyles.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/SearchBar/SearchBar.styled.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/Header/Header.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/Header/Header.styled.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/HomePage/HomePage.js",[],"/Users/jacksonchen/yearone/flix-frontend/src/components/HomePage/HomePage.styled.js",[],"/Users/jacksonchen/y